{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm install && npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "rootDirectory": "web",
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": false,
      "master": false
    }
  },
  "regions": ["icn1"],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "AutomationMaster",
    "NEXT_PUBLIC_APP_VERSION": "1.1.0"
  }
}
